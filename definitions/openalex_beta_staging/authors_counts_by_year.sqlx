config {
  type: "incremental",
  name:"authors_counts_by_year",
  
  
  
}

SELECT  
substring(json_value(authors,"$.id"),22) as author_id,
json_value(authors,"$.year") as year,
json_value(authors,"$.works_count") as works_count,
json_value(authors,"$.cited_by_count") as cited_by_count,
from ${ref("author")},
unnest(json_extract_array(authors,"$.counts_by_year")) as counts_by_year 




